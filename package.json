{
  "name": "reactNativeBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "prettier": "prettier --write src/**/*.js",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "test": "jest",
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "reset-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && yarn cache clean && yarn && yarn start -- --reset-cache",
    "run-ios": "react-native run-ios",
    "run-android": "react-native run-android",
    "android-clean": "cd android && ./gradlew clean && ./gradlew uninstallAll && cd ../",
    "pod-install": "cd ios && pod install && cd ../",
    "init-project": "yarn && react-native link && yarn pod-install"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "formik": "^1.5.8",
    "moment": "^2.24.0",
    "native-base": "^2.13.4",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-native": "0.62.3",
    "react-native-confirmation-code-field": "^3.7.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-emoji": "^1.8.0",
    "react-native-i18n": "^2.0.15",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "0.25.0",
    "react-native-masked-text": "^1.12.5",
    "react-native-navigation": "^3.1.2",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.6.2",
    "react-native-svg-transformer": "^0.13.0",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "styled-components": "^4.3.2",
    "yup": "^0.27.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-jest-enzyme": "^7.0.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.5.1",
    "husky": "^2.2.0",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.6",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.17.0",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
